{
  "family": "my-web-api-task",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["EC2"],
  "containerDefinitions": [
    {
      "name": "my-web-api",
      "image": "938926629525.dkr.ecr.us-east-2.amazonaws.com/my-web-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
